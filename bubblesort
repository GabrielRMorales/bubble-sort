def bubble_sort(arr)
counter=1
while counter>0
 counter=0
  arr.each do |x|
    posx=arr.index(x)
    posxtwo=arr.index(x)+1
    if x>arr[posxtwo].to_i&&posx!=arr.length-1
      temp=x
      arr[posx]=arr[posxtwo]
      arr[posxtwo]=temp
      counter+=1
    end
  end
end
arr
end

bubble_sort([4,5,3,78,2,9,0,2])